{
  "name": "remixaustin-com",
  "version": "2.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --ssr",
    "start": "remix-serve ./build/index.js",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --fix --cache-location ./node_modules/.cache/eslint .",
    "lint-staged": "lint-staged",
    "typecheck": "tsc",
    "clean": "rm -rf ./build ./public/build ./node_modules/.cache ./playwright-report",
    "prestart:mocks": "run-s clean build",
    "start:mocks": "binode --import ./mocks/index.js -- @remix-run/serve:remix-serve build/index.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage && open ./coverage/index.html",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "prepare": "husky install",
    "ts:app": "tsc",
    "ts:e2e": "tsc -b e2e",
    "ts": "run-p ts:*",
    "validate": "run-p test lint ts test:e2e",
    "validate:precommit": "run-s lint:fix format",
    "block-robots": "./tools/workflows/block-robots.sh"
  },
  "dependencies": {
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "daisyui": "2.38.0",
    "front-matter": "^4.0.2",
    "isbot": "^3.7.0",
    "path-esm": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "valibot": "^0.20.1"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.0",
    "@playwright/test": "^1.39.0",
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/mdx": "^2.0.9",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "c8": "^8.0.1",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "msw": "^0.47.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.39.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "remark-frontmatter": "^5.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=8.19.3"
  }
}
