{
  "name": "remixaustin-com",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:posts": "cross-env NODE_ENV=production ts-node blog/buildPosts.ts",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "dev:posts": "cross-env NODE_ENV=development ts-node blog/blogDevServer.ts",
    "clean": "rm -rf ./build ./public/build ./node_modules/.cache ./playwright-report",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "prestart:mocks": "run-s clean build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage && open ./coverage/index.html",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "prepare": "husky install",
    "typecheck": "tsc -b && tsc -b e2e",
    "validate": "run-p test lint typecheck test:e2e",
    "validate:precommit": "run-p lint format:check typecheck",
    "block-robots": "./tools/workflows/block-robots.sh"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@remix-run/node": "^1.7.5",
    "@remix-run/react": "^1.7.5",
    "@remix-run/serve": "^1.7.5",
    "@remix-run/server-runtime": "^1.7.5",
    "bcryptjs": "^2.4.3",
    "daisyui": "2.38.0",
    "front-matter": "^4.0.2",
    "isbot": "^3.5.3",
    "mdx-bundler": "^9.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@playwright/test": "1.28.1",
    "@remix-run/dev": "^1.7.5",
    "@remix-run/eslint-config": "^1.7.5",
    "@tailwindcss/typography": "0.5.7",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/ws": "^8.5.4",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "autoprefixer": "^10.4.11",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "chokidar": "^3.5.3",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "fix-esm": "^1.0.1",
    "husky": "^8.0.2",
    "jsdom": "20.0.3",
    "msw": "^0.47.3",
    "npm-run-all": "^4.1.5",
    "playwright": "1.28.1",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.3.1",
    "remark-mdx-images": "^2.0.0",
    "rimraf": "^5.0.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.23.4",
    "ws": "^8.13.0"
  },
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=8.19.3"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
